    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2"
  }
